{
  "manifest_version": 2,
  "name": "Trello & News",
  "short_name": "TreNews",
  "description": "Trello & News extensions for Chrome",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "alarms", "topSites"],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk=' https://cdnjs.cloudflare.com https://api.trello.com; object-src 'self'",
  "offline_enabled": true,
  "optional_permissions": ["identity"],
  "version": "1.0.0"
}
